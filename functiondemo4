def check_even_number(list1):
  even_number=[]
  odd_number=[]
  for x in list1:
    if x % 2==0:
     even_number.append(x)
    else:
     odd_number.append(x)
  return odd_number
  
 
result=check_even_number([6,8,10,12,14,20])

if len(result)>0: 
  print(result)
else:
  print("sorry no odd number found") 